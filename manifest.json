{

  "description": "Add the ability to add custom emojis to github.",
  "manifest_version": 2,
  "name": "Custom Emoji Addiction",
  "version": "1.0",
  "homepage_url": "https://github.com/kyleroush/Custom-Emoji-addiction",


  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_title": "Custom Emoji Addiction Settings",
    "default_popup": "popup/Settings.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "jquery.js",
        "caret.js",
        "atwho.js",
        "chromeStorage.js",
        "Script.js"
      ],
      "css" : [
        "atwho.css"
      ]
    }
  ]
}
